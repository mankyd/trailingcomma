      (function() {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
      })();

  script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js", type="text/javascript", onload="__$loaded();", async="true")
  if (compressed_static)
    script(src=static_url("/c.js"), type="text/javascript", async="true")
  else
    script(src=static_url("/js/default.js"), type="text/javascript", async="true")
    script(src=static_url("/js/jslint.js"), type="text/javascript", async="true")
