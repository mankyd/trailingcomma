doctype html
html
  head
    title= title
    link(rel="icon", type="image/png", href="/i/favicon.png")
    if (compressed_static)
      link(rel="stylesheet",href=static_url("/c.css"))
    else
      link(rel="stylesheet",href=static_url("/css/bootstrap.min.1.4.0.css"))
      link(rel='stylesheet/less', type="text/css", href=static_url('/css/default.less'))
      script(src="/js/less-1.1.5.min.js", type="text/javascript")
    script.
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-270686-11']);
      _gaq.push(['_trackPageview']);
      var __fq = [];
      var $ = function(f) {
        __fq.push(f);
      };
      var __$loaded = function() {
        for (var i = 0; i < __fq.length; i++) {
          $(__fq[i]);
        }
      };
  body
    div.topbar
      div.topbar-inner
        div.container
          a.brand(href="/") Trailing Comma,
          ul.nav
            li.active
              a(href="/#") home
            li
              a(href="/#about") about
            li
              a(href="/#api") api

  div.container.main
